<refentry xml:id='function.cal-to-jd' xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>cal_to_jd</refname>
  <refpurpose>Desteklenen bir takvimden Jülyen Gün Sayısına dönüşüm yapar</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>cal_to_jd</methodname>
   <methodparam><type>int</type><parameter>takvim</parameter></methodparam>
   <methodparam><type>int</type><parameter>ay</parameter></methodparam>
   <methodparam><type>int</type><parameter>gun</parameter></methodparam>
   <methodparam><type>int</type><parameter>yil</parameter></methodparam>
  </methodsynopsis>
  <para>
   <function>cal_to_jd</function> işlevi <parameter>calendar</parameter>
   bağımsız değişkenine göre Jülyen Gün Sayısını hesaplar.
   Desteklenen <parameter>takvim</parameter> türleri
   <constant>CAL_GREGORIAN</constant>,
   <constant>CAL_JULIAN</constant>,
   <constant>CAL_JEWISH</constant> ve
   <constant>CAL_FRENCH</constant>&apos;dir.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>takvim</parameter></term>
     <listitem>
      <para>
       Dönüşüm yapılacak takvim
       <constant>CAL_GREGORIAN</constant>,
       <constant>CAL_JULIAN</constant>,
       <constant>CAL_JEWISH</constant> veya
       <constant>CAL_FRENCH</constant> değerlerinden biri olabilir.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>ay</parameter></term>
     <listitem>
      <para>
       Sayı olarak ay değeri, geçerli olan değer aralığı
       <parameter>takvim</parameter> bağımsız değişkenine bağlıdır
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>gun</parameter></term>
     <listitem>
      <para>
       Sayı olarak gün değeri, gerçerli olan değer aralığı
       <parameter>takvim</parameter> bağımsız değişkenine bağlıdır
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>yil</parameter></term>
     <listitem>
      <para>
       Sayı olarak yıl değeri, geçerli olan değer aralığı
       <parameter>takvim</parameter> bağımsız değişkenine bağlıdır
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Jülyen Gün Sayısı.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>cal_from_jd</function></member>
    <member><function>frenchtojd</function></member>
    <member><function>gregoriantojd</function></member>
    <member><function>jewishtojd</function></member>
    <member><function>juliantojd</function></member>
    <member><function>unixtojd</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

